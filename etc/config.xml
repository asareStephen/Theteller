<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <theteller_payment>
                <active>1</active>
                <model>Payswitch\Theteller\Model\Checkout</model>
                <title>PaySwitch|TheTeller</title>
                <order_status>processing</order_status>
                <inline>0</inline>
                <vendor_id />
                <hash_key backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <cctypes>AE,VI,MC,DI,JCB,DC</cctypes>
                <allowspecific>0</allowspecific>
                <min_order_total>0.01</min_order_total>
                <redirect_url>theteller/standard/redirect</redirect_url>
                <return_url>theteller/standard/response</return_url>
                <cancel_url>theteller/standard/cancel</cancel_url>
               
            </theteller_payment>
        </payment>
    </default>
</config>
